<?xml version="1.0"?>
<config>
    <modules>
        <FACTFinder_Tagcloud>
            <version>0.1.0</version>
        </FACTFinder_Tagcloud>
    </modules>
    <global>
        <models>
            <factfinder_tagcloud>
                <class>FACTFinder_Tagcloud_Model</class>
            </factfinder_tagcloud>
        </models>
        <blocks>
            <factfinder_tagcloud>
                <class>FACTFinder_Tagcloud_Block</class>
            </factfinder_tagcloud>
        </blocks>
        <helpers>
            <factfinder_tagcloud>
                <class>FACTFinder_Tagcloud_Helper</class>
            </factfinder_tagcloud>
        </helpers>
    </global>
    <frontend>
        <layout>
            <updates>
                <factfinder_tagcloud>
                    <file>factfinder/tagcloud.xml</file>
                </factfinder_tagcloud>
            </updates>
        </layout>
    </frontend>
</config>